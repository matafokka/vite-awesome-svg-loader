{
  "private": true,
  "scripts": {
    "build": "npm run turbo-run -- build",
    "build:packages": "npm run turbo-run -- build --filter ./packages/*",
    "build:apps": "npm run turbo-run -- build --filter ./apps/*",
    "build:docs": "npm run turbo-run -- build --filter docs",
    "build:demos": "npm run turbo-run -- build --filter ./demos/*/*",
    "predev": "npm run build:packages",
    "dev": "turbo run dev",
    "dev:docs:standalone": "turbo run dev --filter docs",
    "generate-sources": "node scripts/generate-sources-command.mjs",
    "turbo-run": "node scripts/turbo-run.mjs",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\""
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@swc/core": "^1.3.99",
    "@tsconfig/node18": "^18.2.2",
    "@types/debounce": "^1.2.3",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.3",
    "tsconfig": "*",
    "tsdown": "^0.19.0-beta.2",
    "turbo": "^2.5.6",
    "typescript": "~5.9.3",
    "unplugin-dts": "^1.0.0-beta.6",
    "vite": "^7.3.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vue": "^3.5.13",
    "vue-tsc": "^3.2.1"
  },
  "name": "vite-awesome-svg-loader",
  "packageManager": "npm@8.19.3",
  "workspaces": [
    "apps/*",
    "packages/*",
    "scripts/*",
    "demos/*/*",
    "demos/all"
  ],
  "dependencies": {
    "css-tree": "^2.3.1",
    "debounce": "^1.2.1",
    "imurmurhash": "github:jensyt/imurmurhash-js",
    "svgo": "^3.0.2"
  }
}
