{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:packages": "turbo run build --filter ./packages/*",
    "build:apps": "turbo run build --filter ./apps/*",
    "build:vue": "turbo run build --filter ./apps/vue-demo",
    "build:vanilla": "turbo run build --filter ./apps/vanilla-demo",
    "build:react": "turbo run build --filter ./apps/react-demo",
    "dev": "turbo run dev",
    "dev:vue": "turbo run dev --filter ./apps/vue-demo",
    "dev:vanilla": "turbo run dev --filter ./apps/vanilla-demo",
    "dev:react": "turbo run dev --filter ./apps/react-demo",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,vue}\""
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
