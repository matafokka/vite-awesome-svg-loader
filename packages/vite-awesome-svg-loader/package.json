{
  "type": "module",
  "name": "vite-awesome-svg-loader",
  "description": "Imports SVGs as source code, base64 and data URI. Preserves stroke width, replaces colors with currentColor or custom colors. Optimizes SVGs with SVGO. Creates SVG sprites.",
  "keywords": [
    "vite",
    "svg",
    "loader",
    "import",
    "image",
    "icon",
    "plugin",
    "vite-svg",
    "vite-loader",
    "vite-plugin",
    "vue",
    "react",
    "vanilla",
    "sprite",
    "sprite-sheet",
    "svg-sprite",
    "sprites",
    "sprite-sheets",
    "preserve-line-width",
    "preserve-stroke-width",
    "currentColor",
    "replace-colors",
    "vite-awesome-svg-loader",
    "svgo"
  ],
  "version": "4.0.0",
  "homepage": "https://matafokka.github.io/vite-awesome-svg-loader",
  "repository": "https://github.com/matafokka/vite-awesome-svg-loader",
  "license": "LGPL-2.1-or-later",
  "private": true,
  "main": "dist/loader.cjs",
  "module": "dist/loader.js",
  "types": "dist/loader.d.cts",
  "exports": {
    "./integration-utils/styles.css": {
      "import": "./dist/integration-utils-styles.css",
      "require": "./dist/integration-utils-styles.css"
    },
    "./integration-utils": {
      "import": {
        "types": "./dist/integration-utils.d.ts",
        "default": "./dist/integration-utils.js"
      },
      "require": {
        "types": "./dist/integration-utils.d.cts",
        "default": "./dist/integration-utils.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/loader.d.ts",
        "default": "./dist/loader.js"
      },
      "require": {
        "types": "./dist/loader.d.cts",
        "default": "./dist/loader.cjs"
      }
    },
    "./react-integration": {
      "import": {
        "types": "./dist/react-integration.d.ts",
        "default": "./dist/react-integration.js"
      },
      "require": {
        "types": "./dist/react-integration.d.cts",
        "default": "./dist/react-integration.cjs"
      }
    },
    "./vanilla-integration": {
      "import": {
        "types": "./dist/vanilla-integration.d.ts",
        "default": "./dist/vanilla-integration.js"
      },
      "require": {
        "types": "./dist/vanilla-integration.d.cts",
        "default": "./dist/vanilla-integration.cjs"
      }
    },
    "./vue-integration": {
      "import": {
        "types": "./dist/vue-integration.d.ts",
        "default": "./dist/vue-integration.js"
      },
      "require": {
        "types": "./dist/vue-integration.d.cts",
        "default": "./dist/vue-integration.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "./integration-utils": [
        "./dist/integration-utils.d.ts",
        "./dist/integration-utils.d.cts"
      ],
      ".": [
        "./dist/loader.d.ts",
        "./dist/loader.d.cts"
      ],
      "./react-integration": [
        "./dist/react-integration.d.ts",
        "./dist/react-integration.d.cts"
      ],
      "./vanilla-integration": [
        "./dist/vanilla-integration.d.ts",
        "./dist/vanilla-integration.d.cts"
      ],
      "./vue-integration": [
        "./dist/vue-integration.d.ts",
        "./dist/vue-integration.d.cts"
      ]
    }
  },
  "scripts": {
    "build": "tsdown"
  },
  "devDependencies": {
    "integration-utils": "*",
    "loader": "*",
    "react-integration": "*",
    "vanilla-integration": "*",
    "vue-integration": "*"
  },
  "dependencies": {
    "integration-utils": "*",
    "loader": "*",
    "react-integration": "*",
    "vanilla-integration": "*",
    "vue-integration": "*"
  }
}