{
  "name": "vite-awesome-svg-loader",
  "description": "Imports SVGs as source code, base64 and data URI. Preserves stroke width, replaces colors with currentColor. Optimizes SVGs with SVGO. Creates SVG sprites.",
  "keywords": [
    "vite",
    "svg",
    "loader",
    "vite-svg",
    "vite-loader",
    "vite-plugin",
    "sprite",
    "sprite-sheet",
    "sprites",
    "svg-sprite",
    "svg-sprites",
    "spritesheet",
    "spritesheets",
    "sprite-sheets",
    "svg-spritesheets",
    "svg-sprite-sheets",
    "preserve-line-width",
    "preserve-stroke-width",
    "currentColor",
    "vite-awesome-svg-loader",
    "svgo"
  ],
  "version": "1.0.3",
  "homepage": "https://github.com/matafokka/vite-awesome-svg-loader",
  "repository": "https://github.com/matafokka/vite-awesome-svg-loader",
  "license": "LGPL-2.1-or-later",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./integration-utils": {
      "import": "./dist/integration-utils/index.mjs",
      "require": "./dist/integration-utils/index.js",
      "types": "./dist/integration-utils/index.d.ts"
    },
    "./vue-integration": {
      "import": "./dist/vue-integration/index.mjs",
      "require": "./dist/vue-integration/index.js",
      "types": "./dist/vue-integration/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "./integration-utils": [
        "./dist/integration-utils/index.d.ts"
      ],
      "./vue-integration": [
        "./dist/vue-integration/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "node src/build.mjs"
  },
  "dependencies": {
    "loader": "*",
    "integration-utils": "*",
    "vue-integration": "*"
  }
}
